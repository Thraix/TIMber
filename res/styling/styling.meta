[Variables]
gray-1=#323232
gray-2=#424242
gray-3=#525252
gray-4=#626262
primaryColor=#4CAF50
darkPrimaryColor=#4b994e
darkerPrimaryColor=#307c33
textPrimaryColor=#E0E0E0
textSecondaryColor=#757575
transparent=#00000000

[Frame]
width=fill_parent
height=fill_parent
spacing=10
backgroundColor=$gray-2
borderColor=$gray-4

[Docker]
width=fill_parent
height=fill_parent
backgroundColor=#323232
edgeColor=#323232
edgeSize=10
edgeBorder=2
edgeBorderColor=#626262
border=2
borderColor=#626262

[Container]
backgroundColor=$gray-2

[Label]
font=noto:12
textColor=$textPrimaryColor

[TreeView]
width=fill_parent
height=fill_parent
spacing=0
font=noto:12
textColor=$textPrimaryColor
itemPadding=5
hover-itemBackgroundColor=$gray-4
active-itemBackgroundColor=$gray-3
active-itemBorderColor=$gray-4
active-itemBorder=1
active-itemPadding=4

[Button]
width=fill_parent
height=30
backgroundColor=$primaryColor
hover-backgroundColor=$darkPrimaryColor
press-backgroundColor=$darkerPrimaryColor
textColor=$textPrimaryColor
font=noto:16

[TextBox]
width=200
height=20
backgroundColor=$gray-2
borderColor=$primaryColor
border=0 0 1 0
font=noto:16
textColor=$textPrimaryColor
hintTextColor=$textSecondaryColor

[ProgressBar]
width=fill_parent
height=12
backgroundColor=$gray-3
progressColor=$primaryColor
radius=6

[TabButton]
width=100
height=25
font=noto:12
textColor=$textPrimaryColor
backgroundColor=$gray-1
hover-backgroundColor=$gray-2
active-backgroundColor=$gray-2
active-border=0 0 2 0
active-borderColor=$primaryColor
padding="0 5 0 0"

[SplitIcon]
width=30
height=30
backgroundColor=$gray-2
border=5 2 2 2
borderColor=$gray-3

[ColorPicker]
width=48
height=30
backgroundColor=$primaryColor
borderColor=$gray-3
border=1

[Divider]
backgroundColor=$gray-3
width=fill_parent
height=2

[dividerVertical]
width=2
height=fill_parent
vertical=true

[RadioButton]
width=16
height=16
borderRadius=8
backgroundColor=$gray-2
borderColor=$primaryColor
border=2
radius=6
active-backgroundColor=$primaryColor
active-borderColor=$transparent
active-border=0
active-radius=8

[Slider]
backgroundColor=$gray-3
width=fill_parent
height=16
radius=8
indicatorInside=true

[sliderVertical]
width=16
height=1w
vertical=true

[SliderIndicator]
width=16
height=16
backgroundColor=$primaryColor
radius=8

[headerContainer]
width=fill_parent
height=wrap_content
vertical=false
padding=10 5
border=2
spacing=0
borderColor=$gray-4
